{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "declaration": true,
        "downlevelIteration": true,
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "module": "esnext",
        "moduleResolution": "node",
        "outDir": "./dist/types",
        "skipLibCheck": true,
        "sourceMap": true,
        "target": "es5",
        "lib": [
            "DOM",
            "ES2021"
        ],
        "paths": {
            "app-html": ["./packages/app-html"],
            "ui-router": ["./packages/ui-router"]
        }
    }
}